	<select id="countF01Discount" resultClass="java.lang.Integer" parameterClass="java.util.Map">
		SELECT COUNT(*) FROM (
			SELECT DISTINCT RISKCODE, KINDCODE 
			FROM <include refid="Utils.prpinsSchema"/>FIR_PREMCALC_TMPDTL 
			WHERE OID_FIR_PREMCALC_TMP = #oidFirPremcalcTmp# 
			AND KINDCODE NOT IN ('FB1')
		) FTMP
		<isEqual property="joinPremRate" compareValue="Y">
			INNER JOIN FIR_PREMIUM_RATE FPR 
			ON FTMP.RISKCODE = FPR.RISKCODE 
			AND FTMP.KINDCODE = FPR.KINDCODE 
			AND FPR.PARA_TYPE = 'F01_EXT_KINDCODE'
			WHERE FPR.VALID_DATE <![CDATA[<=]]> TO_DATE(#calcDate#,'YYYYMMDD') 
			AND FPR.INVALID_DATE <![CDATA[>=]]> TO_DATE(#calcDate#,'YYYYMMDD')
			AND FPR.VALID_FALG = '1'
		</isEqual>
		
	</select>
	
