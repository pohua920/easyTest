	<!-- mantis：HAS0275，處理人員：DP0706，需求單編號：HAS0275_未送通報信件排除機櫃件# ADD TAA -->
	<select id="selectUndwrtForUnsend" resultMap="cwpUndwrtAnnounceUnsendMap" parameterClass="java.util.Map">
		SELECT T1.* FROM (     
		     SELECT T1.*,INSURED.IDENTIFYNUMBER FROM (   
		        SELECT DISTINCT PLYNO,PECNO,LPAD(TO_NUMBER(TO_CHAR(PSDATE,'YYYY')) - 1911,4,0) || TO_CHAR(PSDATE,'MMDD') AS STARTDATE FROM TLGTXMASTER@TLGPRODNEW
		        WHERE 1 =1
				AND UWDATE IN 
  				<iterate property="uwdateList" conjunction="," open="(" close=")" >
	          		#uwdateList[]#
      			</iterate>
		        AND UWINS = 'C'
		        AND ACTINS IN ('CGA','CPA','TA','QTH','C','CTAA','CTA','QTHO')
		        AND PECTYPE = '1'
		        AND SUBSTR(PLYNO,7,3) NOT IN('TRW','TAX','TAA')
		    )T1
		    LEFT JOIN <include refid="Utils.xchgSchema"/>PRPCINSURED_CONTACT_TMP1@TLGPRODNEW INSURED
		    ON T1.PLYNO = INSURED.POLICYNO
		    WHERE INSURED.INSUREDFLAG = '1'
		)T1
		LEFT JOIN CWP_UNDWRT_ANNOUNCE R
		ON T1.STARTDATE = R.VALDATE
		AND T1.IDENTIFYNUMBER = R.IDNO
		WHERE R.CHECKNO IS NULL
	</select>
	
