<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="APS012Action" extends="BaseAction" namespace="/aps/012">
		<!-- default -->
		<action name="default" class="APS012Action">
			<result>/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
			<result name="input">/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
		</action>
		<action name="lnkGoExecute" class="APS012Action" method="lnkGoExecute">
			<result>/WEB-INF/pages/APS/APS012/APS012A0.jsp</result>
			<result name="input">/WEB-INF/pages/APS/APS012/APS012A0.jsp</result>
		</action>
		<action name="lnkGoEditO180Fix" class="APS012Action" method="lnkGoEditO180Fix">
			<result>/WEB-INF/pages/APS/APS012/APS012E1.jsp</result>
			<result name="input">/WEB-INF/pages/APS/APS012/APS012E1.jsp</result>
			<result name="error">/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
		</action>
		<action name="lnkGoEditO180FixPage" class="APS012Action" method="lnkGoEditO180FixPage">
			<result>/WEB-INF/pages/APS/APS012/APS012U0.jsp</result>
			<result name="returnQuery">/WEB-INF/pages/APS/APS012/APS012E1.jsp</result>
		</action>
		<action name="btnExecute" class="APS012Action" method="btnExecute">
			<result>/WEB-INF/pages/APS/APS012/APS012A0.jsp</result>
		</action>
		<action name="btnQuery" class="APS012Action" method="btnQuery">
			<result>/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
		</action>
		<action name="btnQueryFix180" class="APS012Action" method="btnQueryFix180">
			<result>/WEB-INF/pages/APS/APS012/APS012E1.jsp</result>
		</action>
		<action name="btnUpdateFix180" class="APS012Action" method="btnUpdateFix180">
			<result>/WEB-INF/pages/APS/APS012/APS012U0.jsp</result>
		</action>
		<action name="btnCancelBatch" class="APS012Action" method="btnCancelBatch">
			<result>/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
		</action>
		<action name="btnGenCSV" class="APS012Action" method="btnGenCSV">
			<result>/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
			<result name="success" type="stream">
				<param name="contentType">text/csv</param>
				<param name="inputName">fileInputStream</param>
				<param name="contentDisposition">attachment;filename="csvForBigPolicy_ToFirm.csv"</param>
				<param name="bufferSize">1024</param>
			</result>
		</action>
		<action name="btnGenExcel" class="APS012Action" method="btnGenExcel">
			<result>/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
			<result name="input">/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
			<result name="success" type="stream">
				<param name="contentType">text/vnd.ms-excel</param>
				<param name="inputName">fileInputStream</param>
				<param name="contentDisposition">attachment;filename="defaultCIBFireRenewal.xls"</param>
				<param name="contentLength">${lengthOfMyFile}</param>
				<param name="bufferSize">1024</param>
			</result>
		</action>
		<action name="btnCompareData" class="APS012Action" method="btnCompareData">
			<result>/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
		</action>
		<action name="btnDownloadCompareData" class="APS012Action" method="btnDownloadCompareData">
			<result>/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
			<result name="success" type="stream">
				<param name="contentType">text/vnd.ms-excel</param>
				<param name="inputName">fileInputStream</param>
				<param name="contentDisposition">attachment;filename="defaultCIBDiff.xls"</param>
				<param name="bufferSize">1024</param>
			</result>
			<result name="error">/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
		</action>
		<action name="btnDownloadCompareData2" class="APS012Action" method="btnDownloadCompareData2">
			<result>/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
			<result name="success" type="stream">
				<param name="contentType">text/vnd.ms-excel</param>
				<param name="inputName">fileInputStream</param>
				<param name="contentDisposition">attachment;filename="defaultCIBDiff2.xls"</param>
				<param name="bufferSize">1024</param>
			</result>
			<result name="error">/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
		</action>
		<!-- (主檔)分頁資料中，重新輸入要顯示的頁數 -->
		<action name="txtChangePageIndex" class="APS012Action" method="txtChangePageIndex">
			<result>/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
			<result name="input">/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
		</action>
		
		<!-- (主檔)分頁資料中，重新選擇下拉式選單中更改每頁所顯示的資料筆數 -->
		<action name="ddlPageSizeChanged" class="APS012Action" method="ddlPageSizeChanged">
			<result>/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
			<result name="input">/WEB-INF/pages/APS/APS012/APS012E0.jsp</result>
		</action>
		
		<action name="txtChangePageIndexFix180" class="APS012Action" method="txtChangePageIndexFix180">
			<result>/WEB-INF/pages/APS/APS012/APS012E1.jsp</result>
			<result name="input">/WEB-INF/pages/APS/APS012/APS012E1.jsp</result>
		</action>
		<action name="ddlPageSizeChangedFix180" class="APS012Action" method="ddlPageSizeChangedFix180">
			<result>/WEB-INF/pages/APS/APS012/APS012E1.jsp</result>
			<result name="input">/WEB-INF/pages/APS/APS012/APS012E1.jsp</result>
		</action>
	</package>
</struts>