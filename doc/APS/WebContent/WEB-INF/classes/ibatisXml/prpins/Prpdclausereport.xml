<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="Prpdclausereport">
  
	<select id="selectForEpolicy" resultClass="java.lang.String" parameterClass="java.util.Map">
		select REPORTNO from ccicdms.PRPDCLAUSEREPORT 
		<dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="clausecode">
	    		CLAUSECODE = #clausecode#
	    	</isNotEmpty>
	    </dynamic>
	</select>
</sqlMap>